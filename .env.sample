# Required API keys
NEYNAR_API_KEY=YOUR_NEYNAR_API_KEY
OPENAI_API_KEY=YOUR_OPENAI_API_KEY

# App URL (required for sharing)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Snapchain HTTP API endpoint (required)
SNAPCHAIN_HTTP_API_URL=YOUR_SNAPCHAIN_URL/v1/castsByFid

# Optional - Cloudflare R2 Configuration for share images
# If not configured, sharing will still work but without custom images
R2_ACCOUNT_ID=YOUR_R2_ACCOUNT_ID
R2_ACCESS_KEY_ID=YOUR_R2_ACCESS_KEY_ID
R2_SECRET_ACCESS_KEY=YOUR_R2_SECRET_ACCESS_KEY
R2_BUCKET_NAME=YOUR_BUCKET_NAME
R2_PUBLIC_URL=https://your-bucket-url.r2.dev

